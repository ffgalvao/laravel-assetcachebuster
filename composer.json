    "keywords": ["assets", "cachebuster", "cache", "cdn", "laravel", "laravel-4"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Bernardi",
            "email": "spekkionu@spekkionu.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Spekkionu\\Assetcachebuster\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
